<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.appserver.dao.IUserMapper" >
  <select id="getUserByid" 
  		  resultType="org.appserver.bo.UserBo" parameterType="string" >
		select * from u_user user where id=#{id} limit 1
  </select>
</mapper>